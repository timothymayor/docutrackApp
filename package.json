    "ejs": "^3.0.1",
    "ejs-layout": "^1.0.15",
    "ejs-lint": "^1.0.1",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "fetch": "^1.1.0",
    "morgan": "^1.7.0",
    "node-fetch": "^2.6.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.0-rc.4",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "^2.5.0"
