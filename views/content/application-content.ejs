        <div class="doc-wrapper">
            <div class="container">
                <div id="doc-header" class="doc-header text-center">
                    <h1 class="doc-title"><span aria-hidden="true" class="icon icon_genius"></span> Applications</h1>
                    <div class="meta"><i class="far fa-clock"></i> Last updated: February 20th, 2020</div>
                </div><!--//doc-header-->
                
                <div id="showcase" class="doc-body row">
                    <div class="doc-content col-md-9 col-12 order-1">
                        
                                <!-- Button trigger modal -->
                                <button type="button" class="btn btn-primary my-3" data-toggle="modal" data-target="#createApplication">
                                  Create an App
                                </button>
                                
                                <!-- Modal -->
                                <div class="modal fade" id="createApplication" tabindex="-1" role="dialog" aria-labelledby="createApplicationLabel" aria-hidden="true">
                                  <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                      <div class="modal-header">
                                        <h5 class="modal-title" id="exampleModalLabel">Create New Application</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                          <span aria-hidden="true">&times;</span>
                                        </button>
                                      </div>
                                      <div class="modal-body">
                                                <!--Create new document here-->
                                            <form action="/doc/application/create" method="POST">
                                              <label for="title">Title:</label>
                                              <input type="text" name="title" placeholder="Title of Application" class="form-control" required>
                                              <br><br>
                                              <label for="description">Description:</label>
                                              <textarea name="description" placeholder="Description of Application" class="form-control" required rows="4"></textarea>
                                              <br><br>
                                              <button type="submit" class="form-control btn btn-primary">Create Application</button>
                                            </form>
                                      </div>
                                      <div class="modal-footer">
                                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                                      </div>
                                    </div>
                                  </div>
                                </div>
                        <div class="content-inner">
                            <section id="premium-themes" class="doc-section">
                                
                                
                            <h2 id="theme-start" class="section-title">List of Applications</h2>
                                <div class="section-block">
                                    <div class="row">
                                    <% if (applications.length !== 0){
                                    applications.forEach(function (application){
                                    //if statement
                                    %>
                            
	                                    <div class="col-md-4 col-12 mb-3">
                                            <div class="theme-card">
                                                <a href="/doc/application/<%= application.id %>"><i class="icon icon_genius" alt="screenshot" ></i></a>
                                                <div class="card-block">
                                                    <h4 class="card-title"><%= application.title %></h4>
                                                    <p class="card-text"><%= application.description %></p>
                                                </div><!--//card-block-->
                                                <a class="mask" href="/doc/application/<%= application.id %>"><i class="icon fa fa-search-plus"></i></a>
                                            </div><!--//theme-card-->
                                        </div>
                                        
                                        <% });
                                        } else { %>
                                        <div class="col-12">
                                            <div class="item-inner">
                                                <h3>Oops! No Application here.</h3>
                                            </div><!--//item-inner-->
                                        </div><!--//item-->
                            <% } %>
                                    </div><!--//row-->
                                </div><!--//section-block-->
                            </section><!--//doc-section-->
                            
                           
                                                        
                        </div><!--//content-inner-->
                    </div><!--//doc-content-->
                    <div class="doc-sidebar col-md-3 col-12 order-0 d-none d-md-flex">
                        <div id="doc-nav" class="doc-nav">
                            <nav id="doc-menu" class="nav doc-menu flex-column sticky">
                                <a class="nav-link scrollto" href="#premium-themes">List of Applications</a>
                                <!-- <a class="nav-link scrollto" href="#free-themes">External Apps</a> -->
                            </nav><!--//doc-menu-->
                        </div>
                    </div><!--//doc-sidebar-->
                </div><!--//doc-body-->              
            </div><!--//container-->
        </div><!--//doc-wrapper-->
        
    </div><!--//page-wrapper-->
    <% }  else if (title == 'Application Detail'){ %>
        <div class="col-sm-12 my-5">    
          <div class="">
              <p>Title:  <%= application.title %></p>
              <p>Description:  <%= application.description %></p>
              <a href="/doc/application/<%= application.id %>/update">Edit Application</a> | <a href="/doc/application/<%= application.id %>/delete">Delete Application</a>
              <br>
              <h5 class="text-center">Documents under this application:</h5>
              <section id="premium-themes" class="doc-section">
                  <div class="section-block">
                      <div class="row">
                      <%
                          if (documents.length == 0) {
                      %>
                          <h6 class="text-center col">There are no documentations here. </h6>
                      <% }else {  documents.forEach(function (document){ %>
              
                          <div class="col-md-4 col-12 mb-3">
                              <div class="theme-card">
                                  <a href="/doc/document/<%=document.id%>"><i class="icon icon_book"></i></a>
                                  <div class="card-block">
                                      <h4 class="card-title"><%= document.subject %></h4>
                                      <p class="card-text"><%= document.description %></p>
                                  </div><!--//card-block-->
                                  <a class="mask" href="/doc/document/<%=document.id%>"><i class="icon fa fa-search-plus"></i></a>
                              </div><!--//theme-card-->
                          </div>
                          
                          <% });
                      } %>
                      </div><!--//row-->
                  </div><!--//section-block-->
              </section><!--//doc-section-->
          </div>
        </div>
      <% } %>